rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /clients/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /events/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /logos/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /news/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /seo/{docId} {
      allow read: if true;
      allow write: if request.auth != null;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}